<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:data_templates="clr-namespace:CelesteEngineEditor.DataTemplates"
                    xmlns:converters="clr-namespace:CelesteEngineEditor.UIConverters"
                    xmlns:bindings_kernel="clr-namespace:BindingsKernel;assembly=BindingsKernel"
                    xmlns:user_controls="clr-namespace:CelesteEngineEditor.UserControls">
    <converters:FlipBoolConverter x:Key="FlipBool"/>
    <data_templates:PropertyDataTemplate x:Key="ScriptableObjectDataTemplate" DataType="{x:Type bindings_kernel:ScriptableObject}">
        <user_controls:ScriptableObjectChooser/>
    </data_templates:PropertyDataTemplate>
</ResourceDictionary>